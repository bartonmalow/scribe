    "@types/lodash": "^4.14.117",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.3.4",
    "@types/request": "^2.47.1",
    "@types/yargs": "^11.0.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "should": "^13.2.3",
